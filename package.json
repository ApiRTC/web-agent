{
  "name": "web-agent",
  "version": "0.1.0",
  "homepage": "https://apirtc.github.io/web-agent",
  "private": true,
  "dependencies": {
    "@apirtc/apirtc": "^5.0.17",
    "@apirtc/mui-react-lib": "^1.0.10",
    "@apirtc/react-lib": "^1.1.2",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.11",
    "@mui/material": "^5.14.11",
    "base-64": "^1.0.0",
    "lodash.debounce": "^4.0.8",
    "lodash.merge": "^4.6.2",
    "logrocket": "^6.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "doc": "npx typedoc --out doc src/public-constants.ts src/types.ts",
    "predeploy": "npm run build && npm run doc && cp ./build/index.html ./build/404.html && cp ./sample/sample.html ./build && cp -r ./doc ./build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@apirtc/shared-types": "file:./shared-types",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/base-64": "^1.0.0",
    "@types/jest": "^29.5.5",
    "@types/lodash.debounce": "^4.0.7",
    "@types/lodash.merge": "^4.6.7",
    "@types/node": "^20.8.4",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "gh-pages": "^5.0.0",
    "react-scripts": "^5.0.1",
    "typedoc": "^0.25.1",
    "typescript": "^5.2.2"
  },
  "overrides": {
    "typescript": "^5.2.2"
  }
}
